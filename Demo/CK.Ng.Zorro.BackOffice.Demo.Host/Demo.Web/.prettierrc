{
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSameLine": true,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "angular",
        "printWidth": 173,
        "tabWidth": 4
      }
    },
    {
      "files": "*.less",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}